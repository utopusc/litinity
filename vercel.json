{
	"version": 2,
	"builds": [
	  {
		"src": "package.json",
		"use": "@vercel/node",
		"config": {
		  "installCommand": "pnpm install --no-frozen-lockfile && pnpm install @sveltejs/vite-plugin-svelte@latest svelte@latest unplugin-icons@latest vite@latest vite-plugin-pwa@latest"
		}
	  }
	],
	"headers": [
	  {
		"source": "/(.*)\\.webp",
		"headers": [
		  {
			"key": "Cache-Control",
			"value": "public, max-age=31536000, immutable"
		  },
		  {
			"key": "CDN-Cache-Control",
			"value": "public, max-age=31536000, immutable"
		  },
		  {
			"key": "Vercel-CDN-Cache-Control",
			"value": "public, max-age=31536000, immutable"
		  }
		]
	  }
	],
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "/index.html"
	  }
	]
  }
  